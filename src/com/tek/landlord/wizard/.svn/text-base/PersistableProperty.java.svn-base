package com.tek.landlord.wizard;

import java.util.List;

import com.tek.landlord.domain.Property;
import com.tek.landlord.domain.Unit;

public class PersistableProperty {

    private final Property property;
    private final List<Property> units;

    public PersistableProperty(Property property, List<Property> units) {
        this.property = property;
        this.units = units;
    }

	public List<Property> getUnits() {
		return units;
	}

	public Property getProperty() {
		return this.property;
	}
}
